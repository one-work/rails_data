<% if @data_list.type == 'Datum::DataImport' %>
  <%= button_to({ action: 'new_import' }, class: 'button is-primary') do %>
    <%= svg_tag 'plus' %>
    <span class="ml-1"><%= t('.new_import.title') %></span>
  <% end %>
<% else %>
  <%= button_to({ action: 'new'}, class: 'button is-rounded is-primary') do %>
    <%= svg_tag 'plus' %>
    <span class="ml-1"><%= t('.new.title') %></span>
  <% end %>
<% end %>
